indent":0,"parameters":["米洛"]},{"code":401,"indent":0,"parameters":["「你來了啊」"]},{"code":101,"indent":0,"parameters":["4莉莉&阿亮",0,0,2]},{"code":401,"indent":0,"parameters":["？？？"]},{"code":401,"indent":0,"parameters":["「研究的怎麼樣呢？」"]},{"code":101,"indent":0,"parameters":["People2",1,0,2]},{"code":401,"indent":0,"parameters":["米洛"]},{"code":401,"indent":0,"parameters":["「已經依據你們的需求增加強度了。」"]},{"code":101,"indent":0,"parameters":["People2",1,0,2]},{"code":401,"indent":0,"parameters":["米洛"]},{"code":401,"indent":0,"parameters":["「謝謝你們一如往常的提供研究資金」"]},{"code":101,"indent":0,"parameters":["4莉莉&阿亮",5,0,2]},{"code":401,"indent":0,"parameters":["？？？"]},{"code":401,"indent":0,"parameters":["「沒什麼，你的機器人研究"]},{"code":401,"indent":0,"parameters":["很適合拿來給大小姐訓練。」"]},{"code":101,"indent":0,"parameters":["People2",1,0,2]},{"code":401,"indent":0,"parameters":["米洛"]},{"code":401,"indent":0,"parameters":["「對吧，想不到居然是『慶記集團』"]},{"code":401,"indent":0,"parameters":["認可我的...」"]},{"code":101,"indent":0,"parameters":["4莉莉&阿亮",5,0,2]},{"code":401,"indent":0,"parameters":["？？？"]},{"code":401,"indent":0,"parameters":["「當然，我們信任你的研究，"]},{"code":401,"indent":0,"parameters":["接下來研究結束後，合約就到了。」"]},{"code":213,"indent":0,"parameters":[3,8,true]},{"code":101,"indent":0,"parameters":["4莉莉&阿亮",5,0,2]},{"code":401,"indent":0,"parameters":["？？？"]},{"code":401,"indent":0,"parameters":["「屆時你想做什麼就做什麼吧...」"]},{"code":213,"indent":0,"parameters":[2,1,false]},{"code":101,"indent":0,"parameters":["People2",1,0,2]},{"code":401,"indent":0,"parameters":["米洛"]},{"code":401,"indent":0,"parameters":["「我第一件事情就是要破壞"]},{"code":401,"indent":0,"parameters":["冒險者公會跟學院！」"]},{"code":101,"indent":0,"parameters":["People2",1,0,2]},{"code":401,"indent":0,"parameters":["米洛"]},{"code":401,"indent":0,"parameters":["「讓他們知道危險的時候，"]},{"code":401,"indent":0,"parameters":["讓會長直面那些弱小的新世代學院生，"]},{"code":401,"indent":0,"parameters":["根本沒辦法戰鬥這一個事實..」"]},{"code":101,"indent":0,"parameters":["People2",1,0,2]},{"code":401,"indent":0,"parameters":["米洛"]},{"code":401,"indent":0,"parameters":["「讓他知道事情的嚴重性！"]},{"code":401,"indent":0,"parameters":["讓他顏面掃地！！」"]},{"code":213,"indent":0,"parameters":[3,8,true]},{"code":101,"indent":0,"parameters":["4莉莉&阿亮",0,0,2]},{"code":401,"indent":0,"parameters":["？？？"]},{"code":401,"indent":0,"parameters":["「你要怎麼做我沒意見，好好做事情就好，"]},{"code":401,"indent":0,"parameters":["不要讓慶人大小姐失望。」"]},{"code":101,"indent":0,"parameters":["People2",1,0,2]},{"code":401,"indent":0,"parameters":["米洛"]},{"code":401,"indent":0,"parameters":["「不用你說我也會認真製作"]},{"code":401,"indent":0,"parameters":["...這台『滅絕機』。」"]},{"code":204,"indent":0,"parameters":[2,5,4]},{"code":213,"indent":0,"parameters":[4,1,false]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["白貓"]},{"code":401,"indent":0,"parameters":["「完蛋了，得趕快告訴烈千才行這個情報才行。」"]},{"code":221,"indent":0,"parameters":[]},{"code":223,"indent":0,"parameters":[[0,0,0,0],1,false]},{"code":249,"indent":0,"parameters":[{"name":"Musical1","volume":90,"pitch":100,"pan":0}]},{"code":246,"indent":0,"parameters":[3]},{"code":230,"indent":0,"parameters":[300]},{"code":216,"indent":0,"parameters":[0]},{"code":205,"indent":0,"parameters":[-1,{"list":[{"code":29,"parameters":[4],"indent":null},{"code":40,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":false}]},{"code":505,"indent":0,"parameters":[{"code":29,"parameters":[4],"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":40,"indent":null}]},{"code":314,"indent":0,"parameters":[0,0]},{"code":122,"indent":0,"parameters":[2,2,1,0,1]},{"code":201,"indent":0,"parameters":[0,79,27,20,2,0]},{"code":222,"indent":0,"parameters":[]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":true,"trigger":3,"walkAnime":true}],"x":8,"y":13},
